{
  "version": 1,
  "name": "amrapp",
  "alias":"amrapp",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}